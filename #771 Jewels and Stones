class Solution {
public:
    int numJewelsInStones(string J, string S) 
    {
        map<char, int> j;
        int numJewels = 0;
        
        for(int i = 0; i < J.length(); i++)
        {
            j.insert( {J[i], 1} );
        }
        
        for(int k = 0; k < S.length(); k++)
        {
            if(j.find(S[k]) != j.end())
                numJewels++;
        }
         
        return numJewels;
        
    }
};
